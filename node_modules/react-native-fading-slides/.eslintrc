{
    "extends": [
      "airbnb",
      "prettier"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "import/no-unresolved": "off",
      "import/no-extraneous-dependencies": "off",
      "import/extensions": "off"
    }
}
